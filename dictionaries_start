student_scores = {
  "Harry": 81,
  "Ron": 78,
  "Hermione": 99, 
  "Draco": 74,
  "Neville": 62,
}

student_grades = {}

for names in student_scores:
    if student_scores[names] > 90:
        student_grades[names] = "Outstanding"
    elif student_scores[names] >80 and student_scores[names]<91:
        student_grades[names] = "Exceeds Expectations"
    elif student_scores[names] >70 and student_scores[names] <81:
        student_grades[names] ="Acceptable"
    elif student_scores[names] < 71:
        student_grades[names] = "Fail"            

    


print(student_grades)